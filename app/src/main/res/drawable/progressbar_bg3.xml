<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--  设置背景色  -->
    <item>
        <shape>
            <corners android:radius="10dp"/>
            <solid android:color="@color/jindu3"/>
            <stroke android:color="@color/jindu3" android:width="0dp"/>
        </shape>
    </item>

    <!--  设置进度条颜色  -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dp"/>
                <gradient
                    android:endColor="#55FFB6"
                    android:startColor="#55FFB6" />
            </shape>
        </clip>
    </item>
</layer-list>